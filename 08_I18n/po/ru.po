#: ../src/guessr.c:29
#, c-format
msgid "Please respond to the following queries with '%s' or '%s'\n"
msgstr "Отвечайте на следующие вопросы с помощью '%s' или '%s'\n"

#: ../src/guessr.c:29 ../src/guessr.c:36
msgid "yes"
msgstr "да"

#: ../src/guessr.c:29 ../src/guessr.c:38
msgid "no"
msgstr "нет"

#: ../src/guessr.c:34
#, c-format
msgid "Is your chosen number greater than %d?\n"
msgstr "Ваше загаданное число больше чем %d?\n"

#: ../src/guessr.c:42
#, c-format
msgid "The number you thought of is %d.\n"
msgstr "Загаданное вами число - %d.\n"
